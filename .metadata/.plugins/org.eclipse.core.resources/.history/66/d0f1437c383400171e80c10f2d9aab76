<SCRIPT language="JavaScript">
function Check() {
	if(Write.name.value.length < 1) {
		alert("작성자를 입력하세요");
		Write.name.focus();
			return false;
	}
	if(Write.pass.value.length < 1) {
		alert("비밀번호를 입력하세요");
		Write.name.focus();
			return false;
	}
	
	if(Write.title.value.length < 1) {
		alert("제목을 입력하세요.");
		Write.write_title.focus();
			return false;
	}
	if(Write.contents.value.length < 1) {
		alert("글 내용을 입력하세요.");
		Write.content.focus();
			return false;
	}

Write.submit();
}
</SCRIPT>
